{
  "name": "teledash",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:teledash/teledash.git",
  "author": "danielbh <danielhollcraft@gmail.com>",
  "license": "MIT",
  "scripts": {
    "setup": "yarn install & cd server && yarn install & cd client && yarn install",
    "start:server": "babel-watch services/config_persistence_psql/index.js",
    "start:client": "cd client && yarn start",
    "start": "concurrently 'yarn start:server' 'yarn start:client'",
    "service:config": "babel-watch services/config_persistence_psql/index.js",
    "seed": "babel-node services/config_persistence_psql/seed.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "concurrently": "^3.5.1"
  },
  "dependencies": {}
}
